{
  "name": "Robogo Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editor.selectionBackground": "#264f78",
    "editor.inactiveSelectionBackground": "#3a3d41"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#6a9955"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword.control.robogo",
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": "support.function.robogo",
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Log Levels",
      "scope": "constant.language.robogo",
      "settings": {
        "foreground": "#4ec9b0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Booleans",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "YAML Keys",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "List Markers",
      "scope": "punctuation.definition.list.begin.yaml",
      "settings": {
        "foreground": "#d4d4d4"
      }
    }
  ]
} 