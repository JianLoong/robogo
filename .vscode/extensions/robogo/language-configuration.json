{
    "comments": {
        "lineComment": "#",
        "blockComment": [
            "#region",
            "#endregion"
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string"
            ]
        },
        {
            "open": "'",
            "close": "'",
            "notIn": [
                "string"
            ]
        },
        {
            "open": "${",
            "close": "}",
            "notIn": [
                "string"
            ]
        }
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ],
        [
            "${",
            "}"
        ]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*.*:\\s*$|^\\s*-\\s*[a-zA-Z_][a-zA-Z0-9_]*:\\s*$|^\\s*(if|for|while|then|else):\\s*$",
        "decreaseIndentPattern": "^\\s*-\\s*[a-zA-Z_][a-zA-Z0-9_]*:\\s*$|^\\s*(else|then):\\s*$",
        "indentNextLinePattern": "^\\s*-\\s*[a-zA-Z_][a-zA-Z0-9_]*:\\s*$|^\\s*(if|for|while|then|else):\\s*$"
    },
    "folding": {
        "markers": {
            "start": "^\\s*#\\s*region\\b",
            "end": "^\\s*#\\s*endregion\\b"
        }
    },
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
    "onEnterRules": [
        {
            "beforeText": "^\\s*-\\s*[a-zA-Z_][a-zA-Z0-9_]*:\\s*$",
            "action": {
                "indent": "indent"
            }
        },
        {
            "beforeText": "^\\s*[a-zA-Z_][a-zA-Z0-9_]*:\\s*$",
            "action": {
                "indent": "indent"
            }
        }
    ]
}