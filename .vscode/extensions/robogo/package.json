{
  "name": "robogo",
  "displayName": "Robogo",
  "description": "Comprehensive support for Robogo test automation framework with rich documentation, syntax highlighting, autocomplete, and validation",
  "version": "1.0.0",
  "publisher": "robogo",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Testing",
    "Other"
  ],
  "keywords": [
    "robogo",
    "test",
    "automation",
    "yaml",
    "api",
    "testing",
    "tdm",
    "swift",
    "postgres"
  ],
  "activationEvents": [
    "onLanguage:robogo"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "robogo",
        "aliases": [
          "Robogo",
          "robogo"
        ],
        "extensions": [
          ".robogo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "robogo",
        "scopeName": "source.robogo",
        "path": "./syntaxes/robogo.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "robogo.runTest",
        "title": "Run Robogo Test",
        "category": "Robogo"
      },
      {
        "command": "robogo.listActions",
        "title": "List Robogo Actions",
        "category": "Robogo"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "robogo.runTest",
          "when": "resourceExtname == .robogo"
        },
        {
          "command": "robogo.listActions",
          "when": "resourceExtname == .robogo"
        }
      ],
      "editor/context": [
        {
          "command": "robogo.runTest",
          "when": "resourceExtname == .robogo",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Robogo",
      "properties": {
        "robogo.executablePath": {
          "type": "string",
          "default": "robogo",
          "description": "Path to the Robogo executable"
        },
        "robogo.outputFormat": {
          "type": "string",
          "enum": [
            "console",
            "json",
            "markdown"
          ],
          "default": "console",
          "description": "Default output format for test execution"
        },
        "robogo.showDetailedDocumentation": {
          "type": "boolean",
          "default": true,
          "description": "Show detailed parameter documentation in hover tooltips"
        }
      }
    },
    "snippets": [
      {
        "language": "robogo",
        "path": "./snippets/robogo.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "16.x",
    "@types/js-yaml": "^4.0.5",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/robogo.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/robogo/issues"
  },
  "homepage": "https://github.com/your-org/robogo#readme",
  "license": "MIT"
}