# Robogo Test Environment Variables
# Copy this file to .env and modify values as needed

# PostgreSQL Database Configuration
POSTGRES_USER=robogo_testuser
POSTGRES_PASSWORD=robogo_testpass
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=robogo_testdb

# Alternative variable names for backward compatibility
DB_USER=robogo_testuser
DB_PASSWORD=robogo_testpass
DB_HOST=localhost
DB_PORT=5432
DB_NAME=robogo_testdb

# API Configuration
API_BASE_URL=http://localhost:8000
API_TOKEN=your_api_token_here

# Kafka Configuration
KAFKA_BROKER=localhost:9092
KAFKA_TOPIC=test_topic

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# Spanner Configuration (when not using emulator)
SPANNER_PROJECT_ID=your-project-id
SPANNER_INSTANCE_ID=your-instance-id
SPANNER_DATABASE_ID=your-database-id

# HTTP Authentication
HTTP_AUTH_TOKEN=Bearer your_bearer_token_here
HTTP_API_KEY=your_api_key_here